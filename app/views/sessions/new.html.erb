<h1> Log-in to your GardenKeeper account! </h1>
<br><br>
<%= flash[:error] %>
<br><br>
<%= form_for :user do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %><br><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br><br>
    <%= f.submit "Login to account" %>
    <% end %>